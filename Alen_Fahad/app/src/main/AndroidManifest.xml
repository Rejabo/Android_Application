<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triptogether">

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="com.google.firebase.MESSAGING_EVENT" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon3"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon3"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.triptogether.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.triptogether.Register" />
        <activity android:name="com.example.triptogether.Chat" />
        <activity android:name="com.example.triptogether.Users" />
        <activity android:name="com.example.triptogether.MainActivity" />
        <activity android:name="com.example.triptogether.Search" />
    </application>

</manifest>